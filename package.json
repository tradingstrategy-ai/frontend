    "@wagmi/core": "^2.13.8",
    "bignumber.js": "^9.1.2",
    "cookie": "^0.6.0",
    "d3-array": "^3.2.4",
    "d3-time": "^3.1.0",
    "date-fns": "^4.1.0",
    "dd-trace": "^5.58.0",
    "dequal": "^2.0.3",
    "devalue": "^5.1.1",
    "express": "^4.21.0",
